{
  "name": "magic-lawyer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pkill -f 'next dev' || true && cross-env PORT=9192 next dev --turbopack",
    "dev:mac": "PORT=9192 next dev --turbopack",
    "dev:windows": "set PORT=9192 && next dev --turbopack",
    "dev:with-ngrok": "pkill -f 'next' || true && pkill -f 'ngrok' || true && pkill -f 'node.*magic-lawyer' || true && sleep 2 && concurrently \"npm run dev\" \"ngrok http 9192\"",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "cross-env PORT=9192 next start",
    "lint": "eslint --fix",
    "db:up": "docker compose -f docker-compose.db.yml up -d",
    "db:down": "docker compose -f docker-compose.db.yml down",
    "db:reset": "docker compose -f docker-compose.db.yml down -v && docker compose -f docker-compose.db.yml up -d",
    "db:reset-dev": "npm run db:down && rm -rf prisma/migrations && npm run db:up && PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=\"SIM, quero resetar o banco de desenvolvimento\" npx prisma db push --force-reset --accept-data-loss && npm run prisma:seed && npm run dev",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "setup": "npm install && npm run db:up && npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "setup:dev": "pkill -f 'next' || true && pkill -f 'ngrok' || true && pkill -f 'node.*magic-lawyer' || true && sleep 2 && npm install && npm run db:up && rm -rf prisma/migrations && PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=\"SIM, quero resetar o banco de desenvolvimento\" npx prisma db push --force-reset --accept-data-loss && npm run prisma:seed && npm run dev:with-ngrok",
    "clean": "npm run db:down && rm -rf node_modules && rm -rf .next",
    "stop": "pkill -f 'next' || true && pkill -f 'ngrok' || true && pkill -f 'node.*magic-lawyer' || true",
    "stop:all": "pkill -f 'next' || true && pkill -f 'ngrok' || true && pkill -f 'node.*magic-lawyer' || true && npm run db:down",
    "kill:all": "pkill -f 'next' || true && pkill -f 'ngrok' || true && pkill -f 'node.*magic-lawyer' || true && pkill -f 'concurrently' || true && sleep 3"
  },
